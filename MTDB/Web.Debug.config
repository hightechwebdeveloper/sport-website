         xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="reCaptchaPrivateKey"
         value="6LeRTxcTAAAAADTemqxg56n4vrWxrp_STEIB8r-j"
         xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
